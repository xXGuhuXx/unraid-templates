<?xml version="1.0"?>
<Container version="2">
  <Name>auto-yt-dl</Name>
  <Repository>guhu007/auto-yt-dl</Repository>
  <Registry>https://hub.docker.com/repository/docker/guhu007/auto-yt-dl</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/113444-support-auto-yt-dl/</Support>
  <Project>https://github.com/xXGuhuXx/auto-yt-dl</Project>
  <Overview>auto-yt-dl is used to automatically download new Videos of specific YouTube Channels. It features a Web Gui to add and remove Channels from your watch list. </Overview>
  <Category>Downloaders:</Category>
  <WebUI>http://[IP]:[PORT:5000]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/xXGuhuXx/unraid-templates/main/auto-yt-dl.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/xXGuhuXx/unraid-templates/main/auto-yt-dl.jpg</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1631106562</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>auto-yt-dl is used to automatically download new Videos of specific YouTube Channels. It features a Web Gui to add and remove Channels from your watch list. </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5000</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir/>
      <ContainerDir>/app/Downloads</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/auto-yt-dl</HostDir>
      <ContainerDir>/app/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Labels/>
  <Config Name="Host Port 1" Target="5000" Default="" Mode="tcp" Description="Container Port: 5000" Type="Port" Display="always" Required="false" Mask="false">5000</Config>
  <Config Name="Host Path 1" Target="/app/Downloads" Default="" Mode="rw" Description="This is the destination of the downloaded videos." Type="Path" Display="always" Required="false" Mask="false">/mnt/user/unRaidLars/downloads/k/</Config>
  <Config Name="AppData" Target="/app/data" Default="/mnt/user/appdata/auto-yt-dl" Mode="rw" Description="Used to store data (ex. monitored Channels)" Type="Path" Display="always-hide" Required="false" Mask="false">/mnt/user/appdata/auto-yt-dl</Config>
</Container>
